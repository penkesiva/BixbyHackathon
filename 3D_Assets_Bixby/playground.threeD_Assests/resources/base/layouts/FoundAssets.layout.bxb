// this found-asset-macro name can be anything, you need to refer this from a view, FoundAssets.view for example
layout-macro-def (found-asset-macro) {
  params {
    param (model) { // this is like your variable name
      type (3DModel) // this is type of your variable. [Java] private 3DModel model;
      min (Required) max (One)
    }
  }
  content {
    image-card {
      size (L)
      
      title-area {
        halign (Start)
        slot1 {
          text {
            value ("#{value(model.authorName)}")
            style (Title_M)
          }
        }
        slot2 {
          single-line {
            text {
              value ("From #{value(model.displayName)} â€¢ #{value(model.displayName)} stars")
              style (Detail_L)
            }
          }
        }
      }
      image-url ("[#{value(model.thumbnailUrl)}]")
    }
  }
}